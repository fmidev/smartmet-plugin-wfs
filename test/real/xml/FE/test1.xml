<?xml version="1.0" encoding="UTF-8"?>
<wfs:GetFeature
  service="WFS"
  version="2.0.0"
  xmlns:wfs="http://www.opengis.net/wfs/2.0"
  xmlns:gml="http://www.opengis.net/gml/3.2"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:avi="http://xml.fmi.fi/namespace/aviation-weather/2014/02/15"
  xmlns:omso="http://inspire.ec.europa.eu/schemas/omso/2.0rc3">
  <wfs:Query typeNames="omso:PointTimeSeriesObservation">
    <fes:Filter
      xmlns:gml="http://www.opengis.net/gml/3.2"
      xmlns:fes="http://www.opengis.net/fes/2.0"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
      <fes:And>
        <fes:During>
          <fes:ValueReference></fes:ValueReference>
          <gml:TimePeriod gml:id="TP1">
            <gml:begin>
              <gml:TimeInstant gml:id="TI1">
                <gml:timePosition>2010-07-29T13:00:00Z</gml:timePosition>
              </gml:TimeInstant>
            </gml:begin>
            <gml:end>
              <gml:TimeInstant gml:id="TI2">
                <gml:timePosition>2010-07-29T14:00:00Z</gml:timePosition>
              </gml:TimeInstant>
            </gml:end>
          </gml:TimePeriod>
        </fes:During>
        <fes:BBOX>
          <fes:ValueReference></fes:ValueReference>
          <gml:Envelope srsName="urn:ogc:def:crs:EPSG::4326">
            <gml:lowerCorner>29.50 62.50</gml:lowerCorner>
            <gml:upperCorner>29.80 62.80</gml:upperCorner>
          </gml:Envelope>
        </fes:BBOX>
        <fes:PropertyIsEqualTo>
          <fes:ValueReference
            xmlns:wfs="http://www.opengis.net/wfs/2.0"
            xmlns:avi="http://xml.fmi.fi/namespace/aviation-weather/2014/02/15"
            xmlns:iwxxm="http://icao.int/iwxxm/1.0"
            xmlns:om="http://www.opengis.net/om/2.0"
            xmlns:sams="http://www.opengis.net/samplingSpatial/2.0"
            xmlns:saf="http://icao.int/saf/1.0">
            om:observedProperty/@xlink:href
          </fes:ValueReference>
          <fes:Literal>http://beta.fmi.fi/fmi-apikey/foobar/meta?observableProperty=observation&amp;param=t2m&amp;language=eng</fes:Literal>
        </fes:PropertyIsEqualTo>
      </fes:And>
    </fes:Filter>
  </wfs:Query>
</wfs:GetFeature>
