  <simpleType name="TechnicalStatusValueType">
    <annotation>
      <documentation>-- Name --
technical status value

-- Definition --
Description of the technical status of administrative boundaries.</documentation>
    </annotation>
    <restriction base="string">
      <enumeration value="edgeMatched">
